UPDATE "main"."users" set "username" = '{{ XUI_DASHBOARD_LOGIN }}';
UPDATE "main"."users" set "password" = '{{ XUI_DASHBOARD_PASSWORD }}';

UPDATE "main"."settings" set "value" = '/etc/letsencrypt/live/{{ DOMAIN }}/fullchain.pem' WHERE "key" = 'webCertFile';
UPDATE "main"."settings" set "value" = '/etc/letsencrypt/live/{{ DOMAIN }}/privkey.pem' WHERE "key" = 'webKeyFile';
UPDATE "main"."settings" set "value" = '/etc/letsencrypt/live/{{ DOMAIN }}/fullchain.pem' WHERE "key" = 'subCertFile';
UPDATE "main"."settings" set "value" = '/etc/letsencrypt/live/{{ DOMAIN }}/privkey.pem' WHERE "key" = 'subKeyFile';

UPDATE "main"."settings" SET "value" = '{{ DOMAIN }}' WHERE "key" = 'webDomain';

UPDATE "main"."settings" SET "value" = 'true' WHERE "key" = 'subEnable';
UPDATE "main"."settings" SET "value" = 'true' WHERE "key" = 'subEncrypt';
UPDATE "main"."settings" SET "value" = 'true' WHERE "key" = 'subShowInfo';
UPDATE "main"."settings" SET "value" = '/{{ XUI_SUB_PATH }}/' WHERE "key" = 'subPath';
UPDATE "main"."settings" SET "value" = '/{{ XUI_SUB_JSON_PATH }}/' WHERE "key" = 'subJsonPath';

UPDATE "main"."settings" SET "value" = '{{ XUI_DASHBOARD_PORT }}' WHERE "key" = 'webPort';
UPDATE "main"."settings" SET "value" = '{{ XUI_SUB_PORT }}' WHERE "key" = 'subPort';

UPDATE "main"."inbounds" SET "port" = '{{ XUI_SS2022_PORT }}' WHERE "remark" = 'SS-2022';
UPDATE "main"."inbounds" SET "port" = '{{ XUI_VLESS_PORT }}' WHERE "remark" = 'VLESS';
UPDATE "main"."inbounds" SET "port" = '{{ XUI_WG_PORT }}' WHERE "remark" = 'WG';